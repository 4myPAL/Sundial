<?xml version = "1.0" ?>
<job-scheduling-data>
<schedule>
    <!-- job with simple trigger -->
    <job>
        <name>ServiceListenerJob</name>
        <job-class>org.knowm.sundial.jobs.SampleJob2</job-class>
        <job-data-map>
            <entry>
                <key>listernerTime</key>
                <value>20000</value>
            </entry>
        </job-data-map>
    </job>
    <trigger>
        <simple>
            <name>ServiceListenerJob-Trigger</name>
            <job-name>ServiceListenerJob</job-name>
            <repeat-count>-1</repeat-count>
            <repeat-interval>20000</repeat-interval>
        </simple>
    </trigger>
</schedule>
</job-scheduling-data>